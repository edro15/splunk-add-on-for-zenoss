
[zenoss_event]
param.cafile = <string> CA File.
param.credential_realm = <string> Credential Realm.
param.web_address = <string> Zenoss Web Interface. Required parameter.
param.credential_app_context = <string> Credential App Context. Default value is TA-zenoss.
param.credential_account = <string> Credential Account. Required parameter.
param.no_ssl_cert_check = <int> Disable SSL Verification. Default value is 0.
param.splunk_server_name = <string> Splunk Server Name. Required parameter.
param.proxy_uri = <string> Proxy URI. Optional parameter.
param.proxy_credential_account = <string> Proxy Credential Account Name. Optional parameter. 
param.proxy_credential_realm = <string> Proxy Credential Realm. Optional parameter.